{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "regions": ["sfo1"],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "^/(?!api).*",
      "dest": "/api/index.js"
    },
    {
      "src": "^/api/.*",
      "dest": "/api/index.js"
    }
  ]
}
