{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "express",
  "functions": {
    "server/index.ts": {
      "runtime": "nodejs20.x",
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/index.js"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
